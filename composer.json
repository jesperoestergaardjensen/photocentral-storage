{
    "name": "jesperoestergaardjensen/photocentral-storage",
    "description": "Repository that define photo storage(s) for PhotoCentral Main",
    "license": "gpl-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-exif": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "PhotoCentralStorage\\": "src/",
            "PhotoCentralStorage\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhotoCentralStorage\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "jesperoestergaardjensen/exif-samples": "1.0.0",
        "jesperoestergaardjensen/photocentral-simple-linux-storage": "dev-main"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
